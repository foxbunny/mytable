{
  "RateLimiterOptions": {
    "Enabled": true,
    "StatusCode": 429,
    "StatusMessage": "Too many requests. Please try again later.",
    "DefaultPolicy": "standard",
    "Policies": [
      {
        "Type": "FixedWindow",
        "Enabled": true,
        "Name": "standard",
        "PermitLimit": 60,
        "WindowSeconds": 60,
        "QueueLimit": 0,
        "AutoReplenishment": true
      },
      {
        "Type": "FixedWindow",
        "Enabled": true,
        "Name": "auth",
        "PermitLimit": 10,
        "WindowSeconds": 60,
        "QueueLimit": 0,
        "AutoReplenishment": true
      }
    ]
  },
  "Auth": {
    "CookieAuth": true,
    "CookieValidDays": 14,
    "CookieHttpOnly": true
  },
  "StaticFiles": {
    "Enabled": true,
    "RootPath": "public"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=mytable;Username=postgres;Password=postgres;Include Error Detail=true"
  },
  "NpgsqlRest": {
    "UploadOptions": {
      "Enabled": true,
      "DefaultUploadHandler": "file_system",
      "UseDefaultUploadMetadataParameter": true,
      "DefaultUploadMetadataParameterName": "_meta",
      "UploadHandlers": {
        "FileSystemEnabled": true,
        "FileSystemPath": "public/uploads",
        "FileSystemUseUniqueFileName": true,
        "FileSystemCreatePathIfNotExists": true
      }
    },
    "ConnectionString": "Host=localhost;Port=5432;Database=mytable;Username=postgres;Password=postgres;Include Error Detail=true",
    "RequiresAuthorization": false,
    "UrlPathPrefix": "api",
    "EndpointCreated": null,
    "NameConverter": "CamelCase",
    "CommentsMode": "ParseAll",
    "DefaultHttpMethod": "POST",
    "CommentHeaderName": "X-Comment",
    "CommentParameterName": "comment",
    "RequestHeadersParameterName": "headers",
    "RequestHeadersMode": "Ignore",
    "HttpMethodParameterName": null,
    "CommentHeader": {
      "Enabled": true,
      "Format": "OnlyWithComment"
    },
    "LoggingOptions": {
      "LogEndpoint": true,
      "LogConnectionNoticeEvents": true,
      "LogCommands": false,
      "LogAnnotations": true
    },
    "ClientCodeGen": {
      "Enabled": false
    }
  }
}
